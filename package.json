{
  "name": "pax-mobile",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "fix": "prettier --write src/ && next lint --fix",
    "typecheck": "tsc -p . --noEmit",
    "prepare": "husky install",
    "aws-sync": "amplify pull && amplify codegen"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "bash -c tsc"
    ]
  },
  "dependencies": {
    "@aws-amplify/ui-react": "^3.4.1",
    "@emotion/react": "^11.10.0",
    "@emotion/styled": "^11.10.0",
    "@mui/icons-material": "^5.10.2",
    "@mui/material": "^5.10.2",
    "aws-amplify": "^4.3.33",
    "jszip": "^3.10.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "next": "12.2.5",
    "qr-scanner": "^1.4.1",
    "react": "18.2.0",
    "react-device-detector": "^1.7.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-swipeable-views": "^0.14.0",
    "react-webcam": "^7.0.1",
    "short-uuid": "^4.2.0",
    "swr": "^1.3.0",
    "use-long-press": "^2.0.2",
    "zen-observable-ts": "^1.1.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.184",
    "@types/node": "18.7.13",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "@types/react-swipeable-views": "^0.13.1",
    "@welldone-software/why-did-you-render": "^7.0.1",
    "autoprefixer": "^10.4.8",
    "eslint": "8.22.0",
    "eslint-config-next": "12.2.5",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.16",
    "tailwindcss": "^3.1.8",
    "typescript": "4.7.4"
  }
}
